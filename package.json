{
  "name": "waitwhat",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:convex\" \"bun run dev:agent\" \"bun run dev:frontend\"",
    "dev:convex": "npx convex dev",
    "dev:agent": "cd agent && bun run dev",
    "dev:frontend": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build:production": "npx convex deploy --cmd 'vite build'"
  },
